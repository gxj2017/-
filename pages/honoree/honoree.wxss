.turntable_box {
  width: 689rpx;
  height: 531rpx;
  background-image: url(https://wine.gengyouplay.cn/uploads/imgs/b.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  margin: 40rpx auto 0;
}

.integral {
  text-align: center;
  font-size: 32rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-weight: bold;
}

.integral text:nth-of-type(2) {
  color: #ff0048;
}

.jiugongge {
  width: 628rpx;
  height: 430rpx;
  margin: 0 auto;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin-top: 2rpx;
  padding: 2rpx 4rpx;
  box-sizing: border-box;
}

.jiugongge view {
  width: 204rpx;
  height: 139rpx;
  background-image: url(https://wine.gengyouplay.cn/uploads/imgs/d.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.jiugongge view:nth-of-type(5) {
  background-image: url(https://wine.gengyouplay.cn/uploads/imgs/a.png);
}

.jiugongge view image {
  width: 146rpx;
  height: 65rpx;
}

.jiugongge view text {
  font-size: 26rpx;
  font-weight: 400;
  color: rgba(0, 0, 0, 1);
}

.jiugongge view:nth-of-type(5) text:nth-of-type(1) {
  font-size: 39rpx;
  font-weight: 400;
  color: rgba(254, 227, 0, 1);
  text-shadow: 0rpx 3rpx 3rpx rgba(205, 14, 0, 0.75);
}

.jiugongge view:nth-of-type(5) text:nth-of-type(2) {
  display: block;
  width: 127rpx;
  height: 33rpx;
  background: rgba(248, 250, 255, 0.53);
  border-radius: 17rpx;
  text-align: center;
  line-height: 33rpx;
  margin-top: 20rpx;
  color: #fff;
}

.explain >text {
  display: block;
  font-size: 27rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  margin-top: 30rpx;
}
.explain_minelog{
  font-size: 28rpx;
  color: rgba(254, 227, 0, 1);
  text-align: center;
  line-height: 52rpx;
}
.explain_box {
  width: 678rpx;
  /* height: 228rpx; */
  background: rgba(255, 226, 251, 1);
  border-radius: 16rpx;
  margin: 70rpx auto 20rpx;
  position: relative;
  padding: 30rpx;
  box-sizing: border-box;
}

.explain_box .title {
  height: 41rpx;
  font-size: 43rpx;
  font-weight: 1000;
  -webkit-text-fill-color: rgba(167, 6, 229, 1); /*文字的填充色*/
  -webkit-text-stroke: 3rpx #fff;
  position: absolute;
  top: -25rpx;
  left: 30rpx;
}

.explain_box text {
  display: block;
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(21, 21, 21, 1);
  line-height: 50rpx;
}

.chenggong{
  position: fixed;
  z-index: 9999;
  background-color: red;
  height: 200rpx;
}