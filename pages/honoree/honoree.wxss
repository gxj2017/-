page {
  width: 100%;
}

.honoree_box {
  width: 100%;
  height: 100%;
  background-image: url(https://wine.gengyouplay.cn/uploads/imgs/c.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  padding-top: 90rpx;
  box-sizing: border-box;
}

.hiader text {
  display: block;
  text-align: center;
}

.hiader text:nth-of-type(1) {
  font-size: 48rpx;
  font-weight: bold;
  font-style: italic;
  color: rgba(255, 255, 255, 1);
  line-height: 104rpx;
  background: linear-gradient(180deg, rgba(255, 255, 255, 1) 0%, rgba(226, 145, 243, 1) 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}

.hiader text:nth-of-type(2) {
  margin-top: 20rpx;
  font-size: 48rpx;
  font-weight: bold;
  font-style: italic;
  color: rgba(255, 234, 2, 1);
  line-height: 104rpx;
  background: linear-gradient(180deg, rgba(255, 255, 255, 1) 0%, rgba(245, 183, 35, 1) 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}

.hiader text:nth-of-type(3) {
  margin: 30rpx auto 0;
  width: 411rpx;
  height: 62rpx;
  background: rgba(254, 189, 254, 0.2);
  border-radius: 31rpx;
  font-size: 24rpx;
  font-weight: 500;
  font-style: italic;
  color: rgba(254, 189, 254, 1);
  line-height: 62rpx;
}

