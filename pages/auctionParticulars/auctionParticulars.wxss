/* pages/auctionParticulars/auctionParticulars.wxss */
page {
  width: 100%;
  height: 100%;
}
.page{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}
.page_main{
  height: 100%;
  flex: 1;
  overflow-y: auto;
}
.auctionParticulars_box {
  width: 100%;
  height: 623rpx;
  position: relative;
}

.auctionParticulars_box .slides {
  width: 100%;
  height: 100%;
}

.auctionParticulars_box .slides image {
  width: 100%;
  height: 100%;
}

.auctionParticulars_box .time {
  position: absolute;
  bottom: 0;
  z-index: 99999;
  width: 100%;
  height: 78rpx;
  line-height: 78rpx;
  background: rgba(0, 0, 0, 1);
  color: #fff;
  opacity: 0.6;
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  font-size: 32rpx;
}

.Particulars {
  width: 100%;
  background: rgba(255, 255, 255, 1);
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  border-bottom: 30rpx solid #fafafa;
}

.Particulars .title {
  display: block;
  width: 100%;
  font-size: 32rpx;
  font-weight: bold;
  color: rgba(33, 33, 33, 1);
  line-height: 39rpx;
}

.price {
  display: flex;
  justify-content: space-between;
  margin-top: 30rpx;
}

.price .xianjia {
  margin-top: 5rpx;
  font-size: 50rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(217, 54, 82, 1);
}

.yip {
  font-size: 26rpx;
  font-weight: bold;
  color: rgba(133, 133, 133, 1);
  padding-top: 30rpx;
}

.yip text {
  color: #ff6714;
}

.select {
  border-bottom: 30rpx solid #fafafa;
}

.select>view {
  display: flex;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.select .site_wm, .select .specification {
  border-bottom: 2rpx solid #e6e6e6;
}

.select .one {
  display: block;
  width: 100rpx;
  height: 100%;
  font-size: 30rpx;
  font-weight: 500;
  color: rgba(133, 133, 133, 1);
}

.select .one1 {
  display: block;
  width: 280rpx;
  height: 100%;
  font-size: 30rpx;
  font-weight: 500;
  color: rgba(133, 133, 133, 1);
}

.select .two {
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  width: 573rpx;
  overflow: hidden;
}
.select .two text{
  color: #D93652;
}
.select .three {
  display: block;
  width: 50rpx;
}
.all_person{
  padding-top: 30rpx;
}
.all_person .speed {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 30rpx;
}

.all_person .speed text:nth-of-type(1) {
  font-size: 30rpx;
  font-weight: bold;
}

.all_person .speed text:nth-of-type(2) {
  font-size: 28rpx;
  font-weight: bold;
}

.person {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 24rpx 30rpx;
  border-bottom: 2rpx solid rgba(230, 230, 230, 1);
}

.person_xinxi {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.person_xinxi image {
  width: 67rpx;
  height: 67rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}

.person_xinxi text {
  font-size: 30rpx;
  font-weight: 500;
}

.person .jiage {
  font-size: 32rpx;
  font-weight: bold;
  color: rgba(217, 54, 82, 1);
}
.evaluate_box{
  border-top: 30rpx solid #fafafa;
}
.evaluate {
  padding: 0rpx 30rpx;
  box-sizing: border-box;
  display: flex;
  height: 70rpx;
  line-height: 70rpx;
  justify-content: space-between;
  border-bottom: 2rpx solid #e6e6e6;
}

.examine {
  margin-bottom: 5rpx;
}

.evaluate .sp {
  font-size: 30rpx;
  font-weight: bold;
  color: rgba(31, 31, 31, 1);
}

.examine .look {
  font-size: 28rpx;
  font-weight: bold;
  color: rgba(158, 157, 157, 1);
}

.examine .right {
  font-size: 28rpx;
  font-weight: bold;
  color: rgba(158, 157, 157, 1);
}

.comment {
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  display: flex;
}

.comment text {
  display: block;
  height: 34rpx;
  background: rgba(217, 54, 82, 0.3);
  border-radius: 17rpx;
  margin-right: 20rpx;
  font-size: 26rpx;
  font-weight: 500;
  color: rgba(217, 54, 82, 1);
  padding: 4rpx 10rpx;
}

.user_com {
  padding: 20rpx 30rpx;
  box-sizing: border-box;
}

.user_xinxi {
  display: flex;
}

.user_xinxi text {
  height: 26rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(33, 33, 33, 1);
  line-height: 54rpx;
}

.user_xinxi image {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  margin-right: 18rpx;
}

.user_text .text {
  display: block;
  height: 62rpx;
  max-height: 72rpx;
  overflow: hidden;
  font-size: 26rpx;
  font-weight: 400;
  color: rgba(33, 33, 33, 1);
  line-height: 36rpx;
  margin-top: 19rpx;
  padding-bottom: 20rpx;
  border-bottom: 2rpx solid #e6e6e6;
}
.user_text .text:last-of-type {
  border-bottom: 0rpx;
}

.user_text {
  margin-bottom: 20rpx;
}
.image_text{
  border-top: 30rpx solid #fafafa;
}
.image_text .title {
  display: block;
  width: 100%;
  height: 78rpx;
  line-height: 78rpx;
  text-align: center;
  font-size: 30rpx;
  font-weight: bold;
  color: rgba(217, 54, 82, 1);
}
.detail_main{
  width: 100%;
  padding: 30rpx;
  box-sizing: border-box;
}

.detail_main image {
  width: 100%!important;
  margin-bottom: 20rpx;
}

.buy {
  width: 100%;
  height: 96rpx;
  display: flex;
  justify-content: space-between;
  overflow: hidden;
}

.buy_left {
  display: flex;
}

.service, .collect {
  margin: 0 30rpx;
}

.service image, .collect image {
  display: block;
  width: 46rpx;
  height: 46rpx;
  margin-top: 17rpx;
  flex-shrink: 0;
}

.collect text {
  display: block;
  margin-top: -3rpx;
  font-size: 22rpx;
  font-weight: 500;
  color: rgba(42, 42, 42, 1);
}
.service text {
  display: block;
  margin-top: -14rpx;
  font-size: 22rpx;
  font-weight: 500;
  color: rgba(42, 42, 42, 1);
}

.buy_right {
  width: 600rpx;
  height: 96rpx;
  background: rgba(217, 54, 82, 1);
  text-align: center;
  line-height: 96rpx;
  font-size: 30rpx;
  font-weight: 500;
  color: rgba(246, 246, 246, 1);
}
.Product_Parameter_container{
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
  position: absolute;
  top: 0;
}
.Product_Parameter {
  width: 750rpx;
  position: fixed;
  z-index: 999999;
  background-color: #fff;
  bottom: 0;
  padding: 20rpx 26rpx;
  box-sizing: border-box;
}

.shut {
  width: 100%;
  display: flex;
  justify-content: flex-end;
}

.shut text {
  display: block;
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  background: rgba(245, 245, 245, 1);
  text-align: center;
  line-height: 40rpx;
  color: rgba(155, 155, 155, 1);
  font-size: 20rpx;
}

.Product_data {
  display: flex;
  height: 90rpx;
  border-bottom: 2rpx solid rgba(238, 238, 238, 1);
  line-height: 90rpx;
}

.Product_Parameter .title {
  display: block;
  text-align: center;
  font-size: 32rpx;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.Product_data text:nth-of-type(1) {
  display: block;
  width: 170rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(133, 133, 133, 1);
}

.Product_data text:nth-of-type(2) {
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.all_auction {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 100000;
  background: rgba(0, 0, 0, 0.5);
}

.all_auction .reng {
  width: 579rpx;
  height: 773rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 16rpx;
  margin: 155rpx auto 0;
  position: relative;
}

.all_auction .wjshut {
  width: 58rpx;
  height: 58rpx;
  background: rgba(224, 224, 224, 1);
  border-radius: 50%;
  text-align: center;
  line-height: 58rpx;
  position: absolute;
  right: -30rpx;
  top: -30rpx;
}

.reng .hiader {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  border-bottom: 1rpx solid rgba(238, 238, 238, 1);
  font-size: 34rpx;
  font-weight: 500;
  color: rgba(31, 31, 31, 1);
}

.wj_list {
  width: 100%;
  height: 670rpx;
  padding: 0 25rpx;
  box-sizing: border-box;
  Overflow:scroll;
}

.wj_xinxi {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 20rpx 0;
}

.xinxi_left {
  display: flex;
  justify-content: space-between;
}

.xinxi_left image {
  width: 72rpx;
  height: 72rpx;
  border-radius: 50%;
  margin-right: 20rpx;
  margin-top: 5rpx;
}

.xinxi_left text {
  display: block;
}

.xinxi_left text:nth-of-type(1) {
  font-size: 30rpx;
  font-weight: 500;
  color: rgba(0, 0, 0, 1);
}

.xinxi_left text:nth-of-type(2) {
  font-size: 26rpx;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
}

.wj_list .jiage {
  font-size: 32rpx;
  font-weight: bold;
  color: rgba(217, 54, 82, 1);
}

.service{
  border: 0rpx;
  background: #fff;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
button::after{
  border: 0rpx;
}


.site {
  width: 750rpx;
  height: 790rpx;
  position: fixed;
  z-index: 2;
  background-color: #fff;
  bottom: 0;
  padding: 20rpx 26rpx;
  box-sizing: border-box;
  border-bottom: 20rpx solid rgba(245, 245, 245, 1);
}

.shut .title {
  text-align: center;
  font-size: 32rpx;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  margin-right: 246rpx;
}

.site .commodity {
  display: flex;
}

.site .commodity image {
  width: 140rpx;
  height: 140rpx;
  margin: 0 20rpx;
}

.site .commodity text {
  display: block;
}

.site .commodity text:nth-of-type(1) {
  font-size: 30rpx;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.site .commodity text:nth-of-type(2) {
  font-size: 26rpx;
  font-weight: 500;
  color: rgba(133, 133, 133, 1);
  margin-top: 50rpx;
}

.commodity_Sp {
  margin-top: 50rpx;
}

.commodity_Sp>text {
  font-size: 26rpx;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.SpList {
  margin-top: 30rpx;
  display: flex;
  flex-wrap: wrap;
}

.SpList text {
  font-size: 26rpx;
  font-weight: 500;
  color: rgba(217, 54, 82, 1);
  border: 2rpx solid rgba(217, 54, 82, 1);
  border-radius: 4rpx;
  padding: 10rpx;
  margin: 10rpx;
}

.select_sp {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
  margin-top: 30rpx;
}

.select_sp .left text {
  font-size: 26rpx;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.select_sp .left text:nth-of-type(2) {
  color: rgba(217, 54, 82, 1);
  margin-left: 30rpx;
}

.select_sp .right {
  display: flex;
}

.select_sp .right text {
  display: block;
  width: 46rpx;
  height: 46rpx;
  text-align: center;
  line-height: 38rpx;
  border: 1rpx solid #bababa;
  font-weight: 800;
  font-size: 32rpx;
}

.select_sp .right input {
  height: 46rpx;
  width: 60rpx;
  text-align: center;
}

.tk_box {
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: 55;
  background-color: rgba(0, 0, 0, 0.5);
  bottom: 0;
}
.tk_box button {
  position: fixed;
  bottom: 0;
  margin: 0;
  padding: 0;
  width: 100%;
  height: 86rpx;
  line-height: 86rpx;
  background: rgba(217, 54, 82, 1);
  z-index: 99999999;
  border-radius: 0;
  font-size: 32rpx;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}
