page {
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}
.hidden {
  display: none!important;
}
.navGroup {
  width: 750rpx;
	height: 88rpx;
	background-color: #ffffff;
  box-sizing: border-box;
  padding: 0 19rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  color: #858585;
}
.navGroup1 {
  width: 750rpx;
	height: 88rpx;
	background-color: #ffffff;
  box-sizing: border-box;
  padding: 0 19rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 28rpx;
  color: #858585;
  overflow-x: auto;
}
/*隐藏滚动条*/
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.orderBox{
  width: 100%;
  height: 100%;
}
.navItem {
  /* width: 110rpx; */
  padding: 0rpx 30rpx;
  height: 100%;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  white-space: nowrap;
}
.navItem.active {
  color: #d93652;
  border-bottom: 2rpx solid #d93652;
}
.item {
  width: 720rpx;
	height: 382rpx;
  font-size: 24rpx;
	background-color: #ffffff;
	border-radius: 11rpx;
  margin: 20rpx auto 0;
  box-sizing: border-box;
  padding: 23rpx 25rpx 19rpx 17rpx;
}
.item1 {
  width: 720rpx;
	/* height: 382rpx; */
  font-size: 24rpx;
	background-color: #ffffff;
	border-radius: 11rpx;
  margin: 20rpx auto 0;
  box-sizing: border-box;
  padding: 23rpx 25rpx 19rpx 17rpx;
}
.orderNum {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
}
.orderNum >text:nth-child(1){
  /* width: 80%; */
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.orderNum >text:nth-child(2){
  /* width: 20%; */
  text-align: right;
  white-space: nowrap;
  padding-left: 20rpx;
}
.imgInfo>image {
  width: 190rpx;
  height: 182rpx;
}
.imgInfo {
  width: 100%;
  position: relative;
  display: flex;
  margin-top: 24rpx;
}
.imgText {
  display: flex;
  flex-direction: column;
  margin-left: 30rpx;
  flex: 1;
}
.imgText_title{
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
}
.desc {
  width: 340rpx;
  color: #333;
}
.price {
  text-align: right;
}
.pickup_way{
  margin-top: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.num {
  font-size: 28rpx;
  color: #858585;
}
.way{
  font-size: 28rpx;
  color: #d93652;
}
.count {
  width: 100%;
  display: flex;
  justify-content: flex-end;
}
.count>text {
  text-align: right;
}
.btnsBottom {
  display: flex;
  width: 100%;
  margin-top: 15rpx;
  justify-content: flex-end;
}
.btnsBottom>view {
  width: 149rpx;
	height: 53rpx;
	background-color: #ffffff;
	border-radius: 26rpx;
	
  display: flex;
  justify-content: center;
  align-items: center;
}
.btnsBottom1 {
  display: flex;
  width: 100%;
  margin-top: 15rpx;
  justify-content: flex-end;
}
.btnsBottom1>view {
  /* width: 149rpx; */
  padding: 0rpx 20rpx;
	height: 53rpx;
	background-color: #ffffff;
	border-radius: 26rpx;
	
  display: flex;
  justify-content: center;
  align-items: center;
  flex-wrap: nowrap;
}
.cancel {
  color: #858585;
  border: solid 1rpx #a6a6a6;
  margin-left: 20rpx;
}
.cost {
  color: #d93652;
  border: solid 1rpx #d93652;
  margin-left: 21rpx;
}
.status {
  color: #d93652;
}
.dataNull{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.dataNull image{
  width: 200rpx;
  height: 200rpx;
  margin-bottom: 20rpx;
}
.dataNull text{
  font-size: 28rpx;
  color: #666;
}